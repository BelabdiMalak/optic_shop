{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "description": "Your project description here",
  "author": "Your Name",
  "type": "module",
  "electron": "electron .",
  "main": "electron.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "electron": "electron electron.js",
    "package": "electron-builder"
  },
  "devDependencies": {
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "electron": "^32.1.2",
    "electron-builder": "^25.1.7",
    "typescript": "^5.5.3",
    "vite": "^5.4.8"
  },
  "dependencies": {
    "electron-is-dev": "^3.0.1",
    "path": "^0.12.7",
    "sqlite3": "^5.1.7"
  },
"build": {
   "appId": "com.example.your_app",
   "productName": "your_app",
   "directories": {
     "output": "dist_electron"
   },
   "files": [
     "dist/**/*",
     "electron.js"
   ],
   "extraResources": {
     "from": "dist/",
     "to": "app/dist/"
   },
   "win": {
     "target": [
       {
         "target": "nsis",
         "arch": [
           "x64"
         ]
       }
     ]
   },
   "linux": {
     "target": [
       "AppImage",
       "deb",
       "tar.gz"
     ],
     "category": "Utility"
   }
  }
}
